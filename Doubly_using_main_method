#include<stdio.h>
#include<stdlib.h>
#include<string.h>

struct node{
int data;
float data2;
struct node *next;
struct node *pre;
};
typedef struct node Node;
int main(){
Node *first,*second;
first=(Node*)malloc(sizeof(Node));
first->data=3;
first->data2=3.2;
first->next=second;
first->pre=NULL;


second=(Node *)malloc(sizeof(Node));
second->data=5;
second->data=5.5;
second->next=NULL;
second->pre=first;

printf("%d\t%.2f",first->data\n,first->data2);
printf("%d\t%.2f",first->data\n,first->data2);

}
